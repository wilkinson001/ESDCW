<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <context-param>
        <description>database name</description>
        <param-name>db-name</param-name>
        <param-value>xyz</param-value>
    </context-param>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>com.MyServletListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>coursework</servlet-name>
        <servlet-class>com.coursework</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>member</servlet-name>
        <servlet-class>com.member</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>payment</servlet-name>
        <servlet-class>com.payment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>com.admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>com.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>approve_member</servlet-name>
        <servlet-class>com.approve_member</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>approve_claim</servlet-name>
        <servlet-class>com.approve_claim</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>check_balances</servlet-name>
        <servlet-class>com.check_balances</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>claim</servlet-name>
        <servlet-class>com.claim</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>coursework</servlet-name>
        <url-pattern>/coursework</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>member</servlet-name>
        <url-pattern>/member.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>payment</servlet-name>
        <url-pattern>/payment.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>approve_member</servlet-name>
        <url-pattern>/approve_member.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>approve_claim</servlet-name>
        <url-pattern>/approve_claim.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>check_balances</servlet-name>
        <url-pattern>/check_balances.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>claim</servlet-name>
        <url-pattern>/claim.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            20
        </session-timeout>
    </session-config>
</web-app>
